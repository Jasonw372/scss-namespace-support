{
  "name": "scss-namespace-support",
  "displayName": "SCSS Namespace Support",
  "description": "Provides autocompletion for SCSS namespaces from @use statements",
  "version": "0.0.1",
  "publisher": "huanyushi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jasonw372/scss-namespace-support.git"
  },
  "bugs": {
    "url": "https://github.com/Jasonw372/scss-namespace-support/issues"
  },
  "homepage": "https://github.com/Jasonw372/scss-namespace-support#readme",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "scss",
    "sass",
    "namespace",
    "autocomplete"
  ],
  "activationEvents": [
    "onLanguage:scss"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extensionproject.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.24.0"
  }
}
